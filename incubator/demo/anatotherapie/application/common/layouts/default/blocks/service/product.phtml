<?
/**
 * @desc Renders a service's product in a ul/li recursively. 
 * 
 * @author Steven Rosato
 */
foreach ($this->model as $value) {
	if( is_array($value) ) {
		print '<li style="list-style:none"><ul>' . $this->partial(
			'default/blocks/service/product.phtml', 
			array('model' => $value)
		) . '</ul></li>';
	} else {
		print "<li>{$value}</li>";
	}
}
?>